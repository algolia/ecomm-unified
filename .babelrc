{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "transform-react-remove-prop-types",
      {
        "mode": "wrap",
        "ignoreFilenames": ["node_modules"]
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "babel-plugin-strip-invariant"
      ]
    }
  }
}
